{
  "name": "@expense-tracker-ui/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "nx serve",
    "build": "nx build",
    "test:coverage": "nx  run-many --all --target=test --codeCoverage=true --skip-nx-cache --output-style=static --coverageReporters='json,lcov,text,clover'",
    "coverage:aggregate": "istanbul report --dir coverage --include coverage/**/coverage-final.json lcov",
    "coverage:all": "npm run test:coverage && npm run coverage:aggregate",
    "test:all": "nx run-many --all --target=test --configuration=ci",
    "build:stats": "nx build --stats-json",
    "analyze:ui": "webpack-bundle-analyzer dist/expense-tracker-ui/stats.json",
    "generate:api": "cross-env OPENAPI_PATH=${OPENAPI_PATH:../expense-tracker-parent/expense-tracker-core/openapi.json} openapi-generator-cli generate -i $OPENAPI_PATH -g typescript-angular -o ./libs/api/src/lib/generated",
    "e2e": "nx e2e senik-ui-e2e --watch"
  },
  "private": true,
